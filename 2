import java.util.Scanner;
import java.lang.String;
public class ex2
{
    public static void main(String[] args)
    {
        Scanner a=new Scanner(System.in);
        String str;
        System.out.println("輸入:");
        str=a.nextLine();

        System.out.println("轉大寫:"+str.toUpperCase());
    }
}
